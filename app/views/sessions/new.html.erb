<h1>Log in</h1>

<%= form_with url: login_path, local: true do |f| %>
  <div>
    <%= f.label :email, "Email" %>
    <%= f.email_field :email, required: true %>
  </div>
  <div>
    <%= f.label :password, "Password" %>
    <%= f.password_field :password, required: true %>
  </div>
  <div>
    <%= f.submit "Log in" %>
  </div>
<% end %>

<p>Don't have an account? <%= link_to "Sign up", sign_up_path %></p>
