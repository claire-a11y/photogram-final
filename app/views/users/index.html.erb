<h1>List of users</h1>

<table>
  <thead>
    <tr>
      <th>Username</th>
      <th>Private?</th>
      <th></th>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= link_to user.username, user_path(user.username) %></td>
        <td><%= user.private ? "Yes" : "No" %></td>
        <td><%= link_to "Show details", user_path(user.username) %></td>
      </tr>
    <% end %>
  </tbody>
</table>
